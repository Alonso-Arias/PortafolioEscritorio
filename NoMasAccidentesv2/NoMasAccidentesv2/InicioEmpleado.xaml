<Window x:Class="NoMasAccidentesv2.InicioEmpleado"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NoMasAccidentesv2"
        mc:Ignorable="d"
        Title="InicioEmpleado" Height="450" Width="800">
    <Grid>
        <Label Content="Empleado" HorizontalAlignment="Left" FontSize="20" Margin="337,27,0,0" VerticalAlignment="Top"/>
        <Label Name="lblName" Content="{Binding Name}" HorizontalAlignment="Left" Margin="330,146,0,0" VerticalAlignment="Top" FontSize="18" Height="31" Width="138"/>
        <Label Name="lblRut" Content="{Binding Name}" HorizontalAlignment="Left" Margin="330,182,0,0" VerticalAlignment="Top" FontSize="18" Height="30" Width="138" RenderTransformOrigin="0.22,0.537"/>
        <Label Name="lblAddress" Content="{Binding Name}" HorizontalAlignment="Left" Margin="330,212,0,0" VerticalAlignment="Top" FontSize="18" Height="31" Width="138" RenderTransformOrigin="1.076,0.35"/>
        <Label Name="lblEmail" Content="{Binding Name}" HorizontalAlignment="Left" Margin="330,243,0,0" VerticalAlignment="Top" FontSize="18" Height="31" Width="143" RenderTransformOrigin="1.076,0.35"/>

    </Grid>
</Window>
